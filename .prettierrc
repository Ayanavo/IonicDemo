{
    "trailingComma": "es5",
    "tabWidth": 4,
    "experimentalTernaries": true,
    "semi": true,
    "printWidth": 240,
    "singleQuote": false,
    "bracketSpacing": false,
    "bracketSameLine": true,

    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "angular"
            }
        },
        {"files": ["*.json", "*.rc"], "options": {"printWidth": 80}}
    ]
}
